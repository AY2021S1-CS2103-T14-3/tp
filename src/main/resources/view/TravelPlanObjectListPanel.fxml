<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.Tab?>

<TabPane xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
      <Tab text="Activities " closable="false">
            <VBox>
                  <ListView fx:id="activityListView" />
            </VBox>
      </Tab>
      <Tab text="Accommodations " closable="false">
            <VBox>
                  <ListView fx:id="accommodationListView" />
            </VBox>
      </Tab>
      <Tab text="Friends " closable="false">
            <VBox>
                  <ListView fx:id="friendListView" />
            </VBox>
      </Tab>
</TabPane>
